{
	"name": "sycnsettings",
	"displayName": "sycnsettings",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.hello",
		"onCommand:extension.openPATPage",
		"onCommand:extension.editPAT",
		"onCommand:extension.syncSettings"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.hello",
				"title": "sync: Hello"
			},
			{
				"command": "extension.openPATPage",
				"title": "sync: Open Github Personal Access Token Page"
			},
			{
				"command": "extension.editPAT",
				"title": "sync: Add/Edit Github Personal Access Token"
			},
			{
				"command": "extension.syncSettings",
				"title": "sync: Sync Settings to Cloud"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"@types/request": "^2.48.1",
		"debug": "^4.1.1",
		"request": "^2.88.0",
		"strip-json-comments": "^3.0.1"
	}
}
