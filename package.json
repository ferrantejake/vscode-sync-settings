{
	"name": "vscode-sync-settings",
	"displayName": "VSCode Sync Settings",
	"publisher": "Jake",
	"description": "",
	"version": "1.0.0",
	"engines": {
		"node": "^11.2.0",
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.hello",
		"onCommand:extension.openPATPage",
		"onCommand:extension.editPAT",
		"onCommand:extension.syncSettings",
		"onCommand:extension.downloadExtension"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ferrantejake/vscode-sync-settings.git"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openPATPage",
				"title": "VSCode Sync Settings: Open Github Personal Access Token Page"
			},
			{
				"command": "extension.editPAT",
				"title": "VSCode Sync Settings: Add/Edit Github Personal Access Token"
			},
			{
				"command": "extension.syncSettings",
				"title": "VSCode Sync Settings: Sync Settings"
			},
			{
				"command": "extension.downloadExtension",
				"title": "VSCode Sync Settings: Download Extension"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/request": "^2.48.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"debug": "^4.1.1",
		"extract-zip": "^1.6.7",
		"request": "^2.88.0",
		"strip-json-comments": "^3.0.1"
	}
}
